        function unblock(){
            const inp1 = document.getElementById("inp1");
            const inp2 = document.getElementById("inp2");
            const inp3 = document.getElementById("inp3");
            const inp4 = document.getElementById("inp4");
            const inp5 = document.getElementById("inp5");
            const tag_input = document.getElementById("tag-input")
            inp1.classList.remove("blk");
            inp2.classList.remove("blk");
            inp3.classList.remove("blk");
            inp4.classList.remove("blk");
            inp5.classList.remove("blk");
            tag_input.classList.remove("blk");
            const btn_edit=document.getElementById("edit");
            const btn_save=document.getElementById("save");
            btn_edit.classList.add("btn-deactive");
            btn_save.classList.remove("btn-deactive");
        }
        function sendData() {
            const inp1 = document.getElementById("inp1");
            console.log("a");
            console.log(inp1);
            console.log(inp1.value);
            const inp2 = document.getElementById("inp2");
            const inp3 = document.getElementById("inp3");
            const inp4 = document.getElementById("inp4");
            const inp5 = document.getElementById("inp5");
            const tag_input = document.getElementById("tag-input")
            collectValues();
            let obj = {
                uname: inp1.value,
                uemail: inp2.value,
                umajor: inp3.value,
                uyear: inp4.value,
                ulist: tag_list
            }
            console.log(obj);
            const btn_edit=document.getElementById("edit");
            const btn_save=document.getElementById("save");

            btn_save.classList.add("btn-deactive");
            btn_edit.classList.remove("btn-deactive");
            blockall2();
            fetch('/receive-object', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json',
                },
                body: JSON.stringify(obj),
            })
                .then(response => response.text())
                .then(data => {
                    console.log(data); // Response from the server
                })
                .catch(error => {
                    console.error('Error:', error);
                });
        }
        let spanCounter = 0; // To maintain unique IDs for span elements
        let tag_list = [];
        function addSpan() {
            const spanContainer = document.getElementById('span-container');

            // Create a new span element with a unique ID
            const newSpan = document.createElement('span');
            const spanId = `span-${spanCounter}`;
            newSpan.id = spanId;
            newSpan.classList.add("badge", "d-flex", "p-2", "align-items-center", "text-primary-emphasis", "bg-primary-subtle", "border", "border-primary-subtle", "rounded-pill");
            const newElement = document.createElement('span');
            newElement.textContent = document.getElementById("tag-input").value;
            newElement.classList.add('px-1');
            newSpan.appendChild(newElement);

            // Create a delete button for the span
            const deleteButton = document.createElement('a');
